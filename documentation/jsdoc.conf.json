{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"include": ["../index.js", "../src", "./references"],
		"exclude": [],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(docs|node_modules)"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": true
	},
	"opts": {
		"destination": "./html",
		"recurse": true,
		"access": "all"
	}
}